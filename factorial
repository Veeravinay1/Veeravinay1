import java.util.*;

 class Fact
 {
   static int factorial(int x)
   {
     if(x==1)
     return x;
     else
     return x*factorial(x-1);
   }
   public static void main(String args[])
   { 
    Scanner sc=new Scanner(System.in);
    System.out.println("enter a number");
    int n=sc.nextInt(); 
    System.out.println("factorial of "+n+" is "+factorial(n));
   }
 }
